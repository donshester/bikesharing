{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/bikesharing/src",
  "monorepo": true,
  "root": "apps/bikesharing",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/bikesharing/tsconfig.app.json"
  },
  "projects": {
    "bikesharing": {
      "type": "application",
      "root": "apps/bikesharing",
      "entryFile": "main",
      "sourceRoot": "apps/bikesharing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bikesharing/tsconfig.app.json"
      }
    },
    "file-processing": {
      "type": "application",
      "root": "apps/file-processing",
      "entryFile": "main",
      "sourceRoot": "apps/file-processing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-processing/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}